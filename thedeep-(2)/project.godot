; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="FinQuest"
config/version="1.0.0"
run/main_scene="res://scene/startup.tscn"
config/features=PackedStringArray("4.3", "Mobile")
config/icon="res://assets/brand/finQuestIcon.svg"

[autoload]

WaveWipeTransition="*res://scene/wave_wipe_transition.tscn"

[display]

window/size/viewport_width=1600
window/size/viewport_height=720
window/stretch/mode="viewport"
window/stretch/aspect="expand"
window/canvas_textures/default_texture_filter=2

[input_devices]

pointing/emulate_touch_from_mouse=true
pointing/emulate_mouse_from_touch=false

[rendering]

textures/canvas_textures/default_texture_filter=2
renderer/rendering_method="mobile"
renderer/canvas_textures/default_texture_filter=1
renderer/stretch/mode="viewport"
